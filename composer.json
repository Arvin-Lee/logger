{
    "name": "betterde/logger",
    "description": "Elasticsearch logger handler",
    "keywords": ["Elasticsearch", "Logger"],
    "type": "library",
    "version": "1.7.0",
    "require": {
        "laravel/framework": ">=5.6",
        "predis/predis": "^1.1",
        "monolog/monolog": "^1.12 || ^2.0",
        "elasticsearch/elasticsearch": "^7.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "George King",
            "email": "george@betterde.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Betterde\\Logger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Betterde\\Logger\\Providers\\LoggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
